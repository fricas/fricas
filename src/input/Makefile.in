.PHONY: all all-ax all-input

all: all-ax

all-ax all-input: $(OUT) ${FILES} ${FILES2} regress
	@echo 1 finished $(srcdir)

$(OUT):
	$(mkinstalldirs) $@


${OUT}/%.input: %.input
	@ echo generic 10 making ${OUT}/$*.input from $*.input
	cp $*.input ${OUT}/$*.input

.PRECIOUS: %.input

%.input: ${IN}/%.input.pamphlet
	@ echo generic 11 making $*.input from ${IN}/$*.input.pamphlet
	${TANGLE} ${IN}/$*.input.pamphlet > $*.input

${OUT}/%.as: %.as
	@echo generic 12 making ${OUT}/$*.as from $*.as
	cp $*.as ${OUT}/$*.as

%.as: ${IN}/%.as.pamphlet
	@echo generic 13 making $*.as from ${IN}/$*.as.pamphlet
	${TANGLE} ${IN}/$*.as.pamphlet >$*.as

${DOC}/%.input.dvi: ${IN}/%.input.pamphlet ${DOC}/axiom.sty
	@ echo generic 14 making ${DOC}/$*.input.dvi from ${IN}/$*.input.pamphlet
	@ (cd ${DOC} ; \
	cp ${IN}/$*.input.pamphlet ${DOC} ; \
	${DOCUMENT} $*.input ; \
	rm -f ${DOC}/$*.input.pamphlet ; \
	rm -f ${DOC}/$*.input.tex ; \
	rm -f ${DOC}/$*.input )

${DOC}/%.as.dvi: ${IN}/%.as.pamphlet ${DOC}/axiom.sty
	@ echo generic 14 making ${DOC}/$*.as.dvi from ${IN}/$*.as.pamphlet
	@ (cd ${DOC} ; \
	cp ${IN}/$*.as.pamphlet ${DOC} ; \
	${DOCUMENT} $*.as ; \
	rm -f ${DOC}/$*.as.pamphlet ; \
	rm -f ${DOC}/$*.as.tex ; \
	rm -f ${DOC}/$*.as )


use:
	@echo 2 making ${OUT} from ${IN}

regress:
	${MAKE} DAASE=$(axiom_targetdir) regression-tests

bug:
	@echo 4 making ${OUT} from ${IN}

document:
	@echo 5 documenting ${OUT}

mostlyclean-local:
	-rm -f *.output
	-rm -rf $(OUT)

clean-local: mostlyclean-local

distclean-local: clean-local


