)set break resume
)expose UnittestCount UnittestAux Unittest

testsuite "bugs2019"

testcase "'^' in 'sign'"

testEquals("sign(2^x)", "1")

testcase "input form for formal diffs"

f := operator 'f
di2 := D(f(x, y^2), [x, y], [2, 1])
idi2 := di2::InputForm
testEquals("interpret(idi2) - di2", "0")

