           ((OR (ATOM |bfVar#7|) (PROGN (SETQ |i| (CAR |bfVar#7|)) NIL))
            (RETURN NIL))
           (#1#
            (PROGN
             (SETQ |b| (CONCAT (PNAME |i|) " is used in "))
             (|bootOutLines| (SSORT (GETHASH |i| |$bootUsed|)) |stream| |b|))))
          (SETQ |bfVar#7| (CDR |bfVar#7|))))
       (SSORT |a|) NIL)))))
 
