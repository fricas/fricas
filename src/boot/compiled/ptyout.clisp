(DEFUN |bStreamNull| (|x|)
  (PROG (|st|)
    (RETURN
     (COND ((OR (NULL |x|) (EQCAR |x| '|nullstream|)) T)
           (#1='T
            (PROGN
             ((LAMBDA ()
                (LOOP
                 (COND ((NOT (EQCAR |x| '|nonnullstream|)) (RETURN NIL))
                       (#1#
                        (PROGN
                         (SETQ |st| (APPLY (CADR |x|) (CDDR |x|)))
                         (RPLACA |x| (CAR |st|))
                         (RPLACD |x| (CDR |st|))))))))
             (EQCAR |x| '|nullstream|)))))))
 
