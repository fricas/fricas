;          else [x1,cons(f,x2)]
 
(DEFUN |defSeparate| (|x|)
  (PROG (|f| |LETTMP#1| |x1| |x2|)
    (RETURN
     (COND ((NULL |x|) (LIST NIL NIL))
           (#1='T (SETQ |f| (CAR |x|))
            (SETQ |LETTMP#1| (|defSeparate| (CDR |x|)))
            (SETQ |x1| (CAR |LETTMP#1|)) (SETQ |x2| (CADR |LETTMP#1|))
            (COND ((|bfBeginsDollar| |f|) (LIST (CONS |f| |x1|) |x2|))
                  (#1# (LIST |x1| (CONS |f| |x2|)))))))))
 
