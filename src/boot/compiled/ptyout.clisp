     (COND ((NULL |x|) NIL) ((ATOM |x|) (LIST |x|))
           ((AND (CONSP |x|) (EQ (CAR |x|) '&REST)
                 (PROGN
                  (SETQ |ISTMP#1| (CDR |x|))
                  (AND (CONSP |ISTMP#1|) (EQ (CDR |ISTMP#1|) NIL)
                       (PROGN (SETQ |y| (CAR |ISTMP#1|)) #1='T))))
            (LIST |y|))
           (#1# (CONS (CAR |x|) (|unfluidlist| (CDR |x|))))))))
 
