           ((OR (ATOM |bfVar#8|) (PROGN (SETQ |i| (CAR |bfVar#8|)) NIL))
            (RETURN NIL))
           (#1# (HPUT |$bootUsed| |i| (CONS |nee| (GETHASH |i| |$bootUsed|)))))
          (SETQ |bfVar#8| (CDR |bfVar#8|))))
       |$used| NIL)))))
 
; defuse1(e,y)==
;      ATOM y =>
