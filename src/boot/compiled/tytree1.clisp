; bfMKPROGN l==
;     a:=[:bfFlattenSeq c for c in tails l]
;     null a=> nil
;     null CDR a=> CAR a
;     ["PROGN",:a]
 
(DEFUN |bfMKPROGN| (|l|)
  (PROG (|a|)
    (RETURN
     (PROGN
      (SETQ |a|
              ((LAMBDA (|bfVar#27| |c|)
                 (LOOP
