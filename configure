## ------------------------
## -- Which Lisp to use? --
## ------------------------
##
## We will default to GCL later, if no lisp implementation is specified.
fricas_lisp=
fricas_lisp_flavor=unknown

# Check whether --with-lisp was given.
if test "${with_lisp+set}" = set; then
  withval=$with_lisp; fricas_lisp=$withval
fi

## If --enable-gcl is specified, we need to check for coonsistency
axiom_include_gcl=
if test -z $fricas_lisp ; then
    # Check whether --enable-gcl was given.
if test "${enable_gcl+set}" = set; then
  enableval=$enable_gcl; case $enableval in
                       yes|no) axiom_include_gcl=$enableval ;;
                       *) { { echo "$as_me:$LINENO: error: erroneous value for --enable-gcl" >&5
echo "$as_me: error: erroneous value for --enable-gcl" >&2;}
   { (exit 1); exit 1; }; } ;;
                   esac
fi

fi
## We need to build our own GCL is none is avalaible
if test x"$axiom_include_gcl" != xyes ; then
  if test -z $fricas_lisp ; then
    # Extract the first word of "gcl", so it can be a program name with args.
set dummy gcl; ac_word=$2
{ echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6; }
if test "${ac_cv_path_FRICAS_LISP+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $FRICAS_LISP in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_FRICAS_LISP="$FRICAS_LISP" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_FRICAS_LISP="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done
IFS=$as_save_IFS

  ;;
esac
fi
FRICAS_LISP=$ac_cv_path_FRICAS_LISP
if test -n "$FRICAS_LISP"; then
  { echo "$as_me:$LINENO: result: $FRICAS_LISP" >&5
echo "${ECHO_T}$FRICAS_LISP" >&6; }
else
  { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
fi


    fricas_lisp=$FRICAS_LISP
  else
    ## Honor use of Lisp image specified on command line
    FRICAS_LISP=$fricas_lisp

    :
  fi
fi
## Coherence check for GCL inclusion.
case $axiom_include_gcl,$fricas_lisp in
    ,|no,|yes*)
       ## It doesn't make sense not to include GCL when no Lisp image
       ## is available.  Give up.
       if test $axiom_include_gcl,$FRICAS_LISP = no,; then
           { { echo "$as_me:$LINENO: error: --disable-gcl specified but no GCL image found" >&5
echo "$as_me: error: --disable-gcl specified but no GCL image found" >&2;}
   { (exit 1); exit 1; }; }
       fi

       ## No Lisp image was specified and none was available from
       ## the build environment; build GCL from FriCAS source.
       if test -f $axiom_top_srcdir/gcl/configure.in ; then
         FRICAS_LISP='$(axiom_build_bindir)/gcl'
         axiom_all_prerequisites="$axiom_all_prerequisites all-gcl"
         axiom_include_gcl=yes
         fricas_lisp_flavor=gcl
         axiom_fasl_type=o
       else
          { { echo "$as_me:$LINENO: error: GCL and GCL sources missing, see INSTALL" >&5
echo "$as_me: error: GCL and GCL sources missing, see INSTALL" >&2;}
   { (exit 1); exit 1; }; }
       fi
       ;;
    yes,*)
       { { echo "$as_me:$LINENO: error: --with-lisp=$fricas_lisp conflicts with --enable-gcl" >&5
echo "$as_me: error: --with-lisp=$fricas_lisp conflicts with --enable-gcl" >&2;}
   { (exit 1); exit 1; }; }
       ;;
esac


